{
	"name": "@markdown-ast/parser",
	"description": "Markdown parser",
	"version": "0.0.0",
	"license": "MIT",
	"author": {
		"email": "pebrianz117@gmail.com",
		"name": "Pebrian",
		"url": "https://github.com/pebrianz"
	},
	"homepage": "https://github.com/pebrianz/markdown-ast",
	"bugs": "https://github.com/pebrianz/markdown-ast/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/pebrianz/markdown-ast.git"
	},
	"type": "module",
	"scripts": {
		"dev": "pnpm -w dev packages/parser",
		"test:debug": "pnpm -w test:debug packages/parser",
		"test:release": "pnpm -w test:release packages/parser",
		"test": "pnpm test:debug && pnpm test:release",
		"lint": "pnpm -w lint packages/parser --ignore-pattern '**/utils.ts'",
		"asbuild:debug": "asc assembly/index.ts --target debug",
		"asbuild:release": "asc assembly/index.ts --target release",
		"asbuild": "pnpm asbuild:debug && pnpm asbuild:release",
		"build": "pnpm asbuild"
	},
	"files": ["./build", "./assembly"],
	"exports": {
		".": {
			"import": "./build/release.js",
			"types": "./build/release.d.ts"
		},
		"./build/*": "./build/*",
		"./assembly/*": "./assembly/*"
	},
	"devDependencies": {
		"@types/node": "^24.0.4",
		"assemblyscript": "^0.28.4",
		"dedent": "^1.6.0",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	}
}
